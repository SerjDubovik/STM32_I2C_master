#include "stm32f10x.h"
#include "GPIO.h"
			

int main(void)
{

	init_GPIO();

    relay_1(off);
    relay_2(off);

    blue_led(off);
    red_led(off);



    // настройка I2C1

    RCC->APB1ENR |=	RCC_APB1ENR_I2C1EN;		// Тактируем модуль I2C1

    I2C1->CR1 &= ~I2C_CR1_SMBUS;

    I2C2->CR2 &= ~I2C_CR2_FREQ;





	for(;;)
	{


	} // скобка бесконечного цикла

} // скобка мейна
